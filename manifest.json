{
  "name" : "Imageinfo+",
  "version" : "1.0.0",
  "description" : "Get image info for images, Colormap, EXIF data",
  "background_page" : "background.html",
  "permissions" : [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
   ],
  "minimum_chrome_version" : "12.0.0.0",
  "icons" : {
    "16" : "images/imageinfo-16.png",
    "48" : "images/imageinfo-48.png",
    "128" : "images/imageinfo-128.png"
  },
  "content_security_policy": "default-src 'self'; connect-src *; img-src *"

}
